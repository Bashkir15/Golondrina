<!DOCTYPE html>
<html>
	<head>
		<meta charset='utf-8'>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>Golondrina Studio</title>
		<link href="https://fonts.googleapis.com/css?family=Lato:300,400i|Open+Sans:300,300i,400,700" rel="stylesheet">
		<link rel="stylesheet" href="https://unpkg.com/flickity@2/dist/flickity.css">
		<link rel='stylesheet' href='./static/icons/style.css'>
		<link rel='stylesheet' href='./styles/main.min.css'>


	</head>

	<body>
		<% include ../shared/nav %>
		<div class='signup-form'>
			<form>
				<div>
					<input type='text' name='name' required id='signup-name'>
					<label>Name</label>
				</div>

				<div>
					<input type='text' name='username' required id='signup-username'>
					<label>Username</label>
				</div>

				<div>
					<input type='email' name='email' required id='signup-email'>
					<label>Email</label>
				</div>

				<div>
					<input type='password' name='password' required id='signup-password'>
					<label>Password</label>
				</div>

				<div class='auth-form-submit'>
					<button type='button' id='signup-submit'>
						<span>Signup</span>
					</button>
				</div>
			</form>
		</div>

		<script src='./bundle.js'></script>
	</body>
</html>
